{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-fallthrough": 2,
    "default-case": 2,
    "no-console": 0,
    "guard-for-in": 2,
    "no-unused-vars": [
      2,
      {
        "args": "none",
        "vars": "local"
      }
    ],
    "eqeqeq": 2,
    "no-native-reassign": 2
  }
}
